<config>
    <modules>
        <MP_Imageswitch>
            <version>0.1.0</version>
        </MP_Imageswitch>
    </modules>
    <frontend>
        <routers>
            <imageswitch>
                <use>standard</use>
                <args>
                    <module>MP_Imageswitch</module>
                    <frontName>imageswitch</frontName>
                </args>
            </imageswitch>
        </routers>
		<layout>
		    <updates>
		        <imageswitch>
		            <file>imageswitch.xml</file>
		        </imageswitch>
		    </updates>
		</layout>
    </frontend>
    <global>
    </global>
</config>
